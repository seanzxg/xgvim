{
  "explorer.quitOnOpen": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.file.root.template": "[icon] [git] [hidden & 1][root]",
  "explorer.openAction.strategy": "sourceWindow",
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", "expand", "open"],
    "o": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
    "v": "open:vsplit",
    "c": "preview.content"
  },

  // 代码块
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.directories": ["UltiSnips", "~/.config/nvim/snippets"],
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescriptreact": ["typescript"],
    "typescript": ["javascript"]
  },
  //"snippets.textmateSnippetsRoots": [
  //  "/Users/sean/.config/coc/extensions/node_modules/es7-react-js-snippets/lib"
  //],
  // 提示
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  // list
  "list.insertMappings": {
    "<C-n>": "<Down>",
    "<C-p>": "<Up>"
  },
  "list.nextKeymap": "<C-n>",
  "list.previousKeymap": "<C-p>",
  "list.indicator": ">",
  "list.selectedSignText": " ",
  // 代码报错提示
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",
  // eslint
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  //自动格式
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "scss",
    "less",
    "markdown",
    "html",
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "graphql",
    "vue"
  ]
}
